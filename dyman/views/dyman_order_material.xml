<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="dyman_order_material_tree" model="ir.ui.view">
        <field name="name">dyman.order.material.tree</field>
        <field name="model">dyman.order.material</field>
        <field name="arch" type="xml">
            <list>
                <field name="id"/>
                <field name="order_id"/>
                <field name="component_id"/>
                <field name="operation_id"/>
                <field name="material_id"/>
                <field name="quantity"/>         
            </list>
        </field>
    </record>

    <record id="dyman_order_material" model="ir.actions.act_window">
        <field name="name">Order Material</field>
        <field name="res_model">dyman.order.material</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
