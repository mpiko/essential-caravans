<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="dyman_prodline_component_rule_tree" model="ir.ui.view">
        <field name="name">dyman.prodline.component.rule.tree</field>
        <field name="model">dyman.prodline.component.rule</field>
        <field name="arch" type="xml">
            <list>
                <field name="id"/>
                <field name="prodline_attrtype_attrval_id"/>
                <field name="component_id"/>
                <field name="material_id"/>
                <field name="quantity"/>         
            </list>
        </field>
    </record>

    <record id="dyman_prodline_component_rule" model="ir.actions.act_window">
        <field name="name">Component Rule</field>
        <field name="res_model">dyman.prodline.component.rule</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
